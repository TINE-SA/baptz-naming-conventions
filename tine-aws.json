{
  "name": "tine-aws",
  "resources": [
    {
      "name": "ApiGateway::Resource",
      "shortname": "agr",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "ApiGateway::RestApi",
      "shortname": "agra",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "AWS::KeyPair",
      "shortname": "keypair",
      "template": "{shortname}{pc:regionCode}{pc:descriptiveName}"
    },
    {
      "name": "AWS::Lightsail",
      "shortname": "lightsail",
      "template": "{shortname}{pc:regionCode}{uc:azCode}{pc:descriptiveName}"
    },
    {
      "name": "CloudFormation::Stack",
      "shortname": "cfs",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "CloudWatch::Alarm",
      "shortname": "cwa",
      "template": "{shortname}{pc:resourceRef}{pc:descriptiveName}{pc:hilo}"
    },
    {
      "name": "Cognito::IdentityPool",
      "shortname": "cip",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "Cognito::UserPool",
      "shortname": "cup",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "EC2::AMI",
      "shortname": "ami",
      "template": "{shortname}{pc:amiName}-{version}"
    },
    {
      "name": "EC2::Instance",
      "shortname": "ec2i",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "EC2::SecurityGroup",
      "shortname": "ec2sg",
      "template": "{shortname}{pc:vpcName}{pc:descriptiveName}"
    },
    {
      "name": "EC2::Subnet",
      "shortname": "ec2s",
      "template": "{shortname}{pc:regionCode}{uc:azCode}{pc:vpcName}"
    },
    {
      "name": "EC2::VPC",
      "shortname": "ec2vpc",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "IAM::ManagedPolicy",
      "shortname": "iammp",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "IAM::Role",
      "shortname": "iamr",
      "template": "{shortname}{resourceRef}{pc:descriptiveName}"
    },
    {
      "name": "Kinesis::Stream",
      "shortname": "ks",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "KinesisFirehose::DeliveryStream",
      "shortname": "kfds",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "Lambda::Alias",
      "shortname": "la",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "Lambda::Function",
      "shortname": "lf",
      "template": "{shortname}{pc:descriptiveName}"
    },
    {
      "name": "S3::Bucket",
      "shortname": "s3b",
      "template": "{f:randomChars(4)}-{shortname}-{regionCode}-{envCode}-{kc:descriptiveName}"
    }
  ],
  "questions": [
    {
      "name": "regionCode",
      "message": "Region?",
      "type": "list",
      "choices": [
        {
          "name": "eu-west-1 (Ireland)",
          "value": "ew1",
          "short": "ew1"
        },
        {
          "name": "eu-central-1 (Frankfurt)",
          "value": "ec1",
          "short": "ec1"
        }
      ],
      "default": "ew1"
    },
    {
      "name": "azCode",
      "message": "Availability Zone?",
      "type": "list",
      "choices": [
        "a",
        "b",
        "c"
      ]
    },
    {
      "name": "hilo",
      "message": "High or Low?",
      "type": "list",
      "choices": [
        "high",
        "low"
      ]
    },
    {
      "name": "envCode",
      "message": "Environment?",
      "type": "list",
      "choices": [
        {
          "name": "Production",
          "value": "p"
        },
        {
          "name": "Staging",
          "value": "s"
        },
        {
          "name": "Development",
          "value": "d"
        }
      ]
    },
    {
      "name": "descriptiveName",
      "message": "Descriptive name:",
      "type": "input",
      "validate": "validateNotEmpty"
    }
  ]
}
